{
  "name": "fieldcss",
  "version": "1.0.0",
  "description": "Simple CSS framework for simple and high quolity designs.",
  "scripts": {
    "start": "browser-sync start --server --files '**/*'",
    "styles:sass": "node-sass --include-path scss ./src/scss/bundle.scss ./dist/css/bundle.css --output-style compressed",
    "styles:prefix": "postcss ./dist/scss/bundle.css --use autoprefixer -d ./dist/css",
    "build:styles": "npm-run-all -s styles:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suginoki45/fieldcss.git"
  },
  "author": "Glatch",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "browser-sync": "^2.26.3",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.14",
    "stylelint-config-recommended-scss": "^3.2.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "aos": "^3.0.0-beta.6",
    "minireset.css": "0.0.4"
  }
}
